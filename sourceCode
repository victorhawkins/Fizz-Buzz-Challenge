#include <stdio.h>


int main(void)
{
//Create loop that prints numbers 1 through 100
    for (int i = 1; i <= 100; i++)
      {      
//Check if iteration is not a multiple of 3 or 5           
          if (i % 3 != 0 && i % 5 != 0)
          printf("%i\n", i);
//Check if iteration is only a multiple of 3
          if (i % 3 == 0 && i % 5 != 0)
              {    
              printf("Fizz\n");
              }
//Check if iteration is only a multiple of 5
          if (i % 5 == 0 && i % 3 != 0)
              {
               printf("Buzz\n"); 
              }
//Check if iteration is a multiple of 3 and 5 
          if (i % 5 == 0 && i % 3 == 0)
              {
              printf("FizzBuzz\n");  
              }
      {

}}}
